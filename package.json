{
  "name": "sync-note",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "cd api && node dist/api/src/index",
    "api:build": "cd api && node scripts/build",
    "api:build:w": "cd api && node scripts/build --watch",
    "api:dev": "cd api && set NODE_ENV=development && nodemon --exec \"node dist/api/src/index\"",
    "client:dev": "cd client && vite",
    "client:build": "cd client && vue-tsc && vite build",
    "client:preview": "cd client && vite preview",
    "css:build": "cd client && npx tailwindcss -i src/assets/styles/index.css -o src/assets/styles/tailwind.css -w"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "formidable": "^3.5.1",
    "fs-extra": "^11.1.1",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "oh-vue-icons": "^1.0.0-rc3",
    "open": "^10.1.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "chokidar": "^3.5.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
